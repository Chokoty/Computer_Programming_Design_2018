#define _CRT_SECURE_NO_WARNINGS
#include <stdio.h>
#include "gcd.h"
#include <math.h>
int main()
{
	int a, b;

	printf("첫째 자연수를 입력하시오 : ");
	scanf("%d", &a);
	printf("또 다른 자연수를 입력하시오 : ");
	scanf("%d", &b);
	
	printf("%d과 %d의 최대공약수 : %d\n", a, b, GCD(a, b));
	printf("최소공배수 : %d\n", abs(a*b)/GCD(a,b));
	return 0;
}
